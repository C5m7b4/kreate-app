{
  "extends":[
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "rules":{
    "no-console": 0
  },
  "plugins":[
    "import", "jsx-a11y"
  ],
  "parserOptions":{
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures":{
      "jsx": true
    }
  },
  "env":{
    "es6":true,
    "browser": true,
    "node":true
  }
}